wup_bc_cycle1 <- wup_bc_cycle1 %>% mutate(chart_ticks=round((minute(time) - minute(wup_bc_cycle1[1, 'time'])) * 60 + second(time) - second(wup_bc_cycle1[1, 'time']), 0))
